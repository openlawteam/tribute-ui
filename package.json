    "react": "^17.0.1",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.15.4",
    "react-lines-ellipsis": "^0.15.0",
    "react-media": "^1.10.0",
